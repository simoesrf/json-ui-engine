                        "minLength": 3,
                        "maxLenth": 100
                    },
                    "age": {
                        "type": "integer",
                        "title": "Age",
                        "description": "Your age in years",
                        "minimum": 0,
                        "maximum": 100
                    },
                    "gender": {
                        "type": "string",
                        "enum": [
                            "m",
                            "f"
                        ]
                    }
                },
